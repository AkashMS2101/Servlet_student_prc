<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <context-param>
  	<param-name>school</param-name>
  	<param-value>CKS</param-value>
  </context-param>
  
  <servlet>
  	<servlet-name>two</servlet-name>
  	<servlet-class>student.controller.Save_student</servlet-class>
  			<init-param>
  				<param-name>fees</param-name>
  				<param-value>35000</param-value>
  			</init-param>
  	
  </servlet>
  <servlet-mapping>
  	<servlet-name>two</servlet-name>
  	<url-pattern>/studentinfo</url-pattern>
  </servlet-mapping>
  
 
   <servlet>
  	<servlet-name>three</servlet-name>
  	<servlet-class>student.controller.SuccessfullySignIn_Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>three</servlet-name>
  	<url-pattern>/SuccessfullySignedIn</url-pattern>
  </servlet-mapping>
  
    <servlet>
  	<servlet-name>four</servlet-name>
  	<servlet-class>student.controller.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>four</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
